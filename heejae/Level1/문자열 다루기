class Solution {
    public boolean solution(String s) {
        boolean answer = true;
        // 문자열 길이 4혹은 6이 아니라면 false 반환
        if(s.length() != 4 && s.length() !=6) return false;
        
        // 숫자로만 구성되어 있는지
        for(int i=0; i<s.length(); i++) {
            char c = s.charAt(i);
            if(c<'0' || c>'9') return false;
        }
        
        return answer;
    }
}
